{
  "name": "@lherron/agent-spaces",
  "version": "0.1.1",
  "description": "Command line interface for Agent Spaces v2",
  "repository": {
    "type": "git",
    "url": "https://github.com/lherron/agent-spaces-v2"
  },
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "bin": {
    "asp": "./bin/asp.js"
  },
  "exports": {
    ".": {
      "bun": "./src/index.ts",
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    },
    "./core": {
      "types": "./node_modules/@agent-spaces/core/dist/index.d.ts",
      "import": "./node_modules/@agent-spaces/core/dist/index.js"
    },
    "./resolver": {
      "types": "./node_modules/@agent-spaces/resolver/dist/index.d.ts",
      "import": "./node_modules/@agent-spaces/resolver/dist/index.js"
    },
    "./store": {
      "types": "./node_modules/@agent-spaces/store/dist/index.d.ts",
      "import": "./node_modules/@agent-spaces/store/dist/index.js"
    },
    "./materializer": {
      "types": "./node_modules/@agent-spaces/materializer/dist/index.d.ts",
      "import": "./node_modules/@agent-spaces/materializer/dist/index.js"
    },
    "./git": {
      "types": "./node_modules/@agent-spaces/git/dist/index.d.ts",
      "import": "./node_modules/@agent-spaces/git/dist/index.js"
    },
    "./engine": {
      "types": "./node_modules/@agent-spaces/engine/dist/index.d.ts",
      "import": "./node_modules/@agent-spaces/engine/dist/index.js"
    },
    "./claude": {
      "types": "./node_modules/@agent-spaces/claude/dist/index.d.ts",
      "import": "./node_modules/@agent-spaces/claude/dist/index.js"
    },
    "./lint": {
      "types": "./node_modules/@agent-spaces/lint/dist/index.d.ts",
      "import": "./node_modules/@agent-spaces/lint/dist/index.js"
    }
  },
  "files": ["dist", "bin", "node_modules/@agent-spaces"],
  "bundledDependencies": [
    "@agent-spaces/claude",
    "@agent-spaces/core",
    "@agent-spaces/engine",
    "@agent-spaces/git",
    "@agent-spaces/lint",
    "@agent-spaces/materializer",
    "@agent-spaces/resolver",
    "@agent-spaces/store"
  ],
  "scripts": {
    "build": "tsc",
    "typecheck": "tsc --noEmit",
    "test": "bun test",
    "lint": "biome check .",
    "lint:fix": "biome check --write ."
  },
  "dependencies": {
    "@agent-spaces/claude": "*",
    "@agent-spaces/core": "*",
    "@agent-spaces/engine": "*",
    "@agent-spaces/git": "*",
    "@agent-spaces/lint": "*",
    "@agent-spaces/materializer": "*",
    "@agent-spaces/resolver": "*",
    "@agent-spaces/store": "*",
    "chalk": "^5.3.0",
    "commander": "^12.1.0",
    "figures": "^6.1.0",
    "ora": "^9.0.0"
  },
  "devDependencies": {
    "@types/bun": "latest",
    "typescript": "^5.8.3"
  }
}
